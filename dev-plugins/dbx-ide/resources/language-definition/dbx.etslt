syntax(Model) "resources/language-definition/dbx.ecore" {

	Model:element(Model) -> (Import:composite(imports))* (BaseClassOrSpecial:composite(classes))*;
	Import:element(Import) -> "#import" STRINGDEF:composite(file);
	
	BaseClassOrSpecial -> BaseClass;
	
	BaseClass:element(BaseClass) -> (BaseClassIsAbstract:composite(abstract))?
		"class" NamedElementName "{" (BaseClassContent)* "}";	
		
	BaseClassIsAbstract:constant("true":EBoolean) -> "abstract";
	BaseClassContent -> ";";
	
	NamedElementName -> IDENTIFIER:composite(name);	

	token () {
		"`cstylecomment`";
	}

}