<?xml version="1.0" encoding="ASCII"?>
<dbl:Model xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dbl="http://www.informatik.hu-berlin.de/sam/dbl">
  <modules name="semaphore_app">
    <classifiers xsi:type="dbl:Interface" name="Object"/>
    <classifiers xsi:type="dbl:Clazz" name="FifoSemaphore" active="true">
      <attributes name="count">
        <primitiveType xsi:type="dbl:IntType"/>
        <initialValue xsi:type="dbl:IntLiteral" value="1"/>
      </attributes>
      <attributes name="fifoList" isList="true">
        <listDims>
          <classifierTypeExpr referencedElement="//@modules.0/@classifiers.0"/>
        </listDims>
      </attributes>
      <attributes name="next">
        <classifierTypeExpr referencedElement="//@modules.0/@classifiers.0"/>
      </attributes>
      <methods name="take">
        <primitiveType xsi:type="dbl:VoidType"/>
        <statements xsi:type="dbl:WhileStatement">
          <condition xsi:type="dbl:And">
            <op1 xsi:type="dbl:Equal">
              <op1 xsi:type="dbl:IdExpr" referencedElement="//@modules.0/@classifiers.1/@attributes.0"/>
              <op2 xsi:type="dbl:IntLiteral"/>
            </op1>
            <op2 xsi:type="dbl:NotEqual">
              <op1 xsi:type="dbl:IdExpr" referencedElement="//@modules.0/@classifiers.1/@attributes.2"/>
              <op2 xsi:type="dbl:IdExpr">
                <predefinedId xsi:type="dbl:MeLiteral"/>
              </op2>
            </op2>
          </condition>
          <whileBlock>
            <statements xsi:type="dbl:Wait"/>
          </whileBlock>
        </statements>
        <statements xsi:type="dbl:Assignment">
          <variable>
            <idExpr referencedElement="//@modules.0/@classifiers.1/@attributes.2"/>
          </variable>
          <value xsi:type="dbl:IdExpr">
            <parentIdExpr referencedElement="//@modules.0/@classifiers.1/@attributes.1"/>
            <predefinedId xsi:type="dbl:FirstInSet"/>
          </value>
        </statements>
      </methods>
      <constructor>
        <parameters name="initCount">
          <primitiveType xsi:type="dbl:IntType"/>
        </parameters>
      </constructor>
    </classifiers>
  </modules>
</dbl:Model>
