        </entities>
        <start>
          <actions xsi:type="dbl:SemaphoreTake" semaphore="//@modules.0/@extensions.0/@entities.3">
            <timeout xsi:type="dbl:SemTimeoutNoWait"/>
          </actions>
          <actions xsi:type="dbl:SemaphoreTake" semaphore="//@modules.0/@extensions.0/@entities.3">
            <timeout xsi:type="dbl:SemTimeoutNoWait">
              <semAction xsi:type="dbl:SemActionOK">
                <actions xsi:type="dbl:Task">
                  <codeBlock concreteSyntax="{ trace(&quot;take SEM OK&quot;); }">
                    <statements xsi:type="dbl:ExpressionStatement" concreteSyntax="trace(&quot;take SEM OK&quot;)">
                      <expression xsi:type="dbl:ProcedureCall">
                        <procedureAccess xsi:type="dbl:IdExpr" concreteSyntax="trace(&quot;take SEM OK&quot;)" referencedElement="//@modules.0/@procedures.0">
                          <arguments>
                            <arguments xsi:type="dbl:StringLiteral" concreteSyntax="&quot;take SEM OK&quot;" value="take SEM OK"/>
                          </arguments>
                        </procedureAccess>
                      </expression>
                    </statements>
                  </codeBlock>
                </actions>
                <actions xsi:type="dbl:SemaphoreGive" semaphore="//@modules.0/@extensions.0/@entities.3"/>
              </semAction>
              <semAction xsi:type="dbl:SemActionError">
                <actions xsi:type="dbl:Task">
                  <codeBlock concreteSyntax="{ trace(&quot;take SEM ERROR&quot;); }">
                    <statements xsi:type="dbl:ExpressionStatement" concreteSyntax="trace(&quot;take SEM ERROR&quot;)">
                      <expression xsi:type="dbl:ProcedureCall">
                        <procedureAccess xsi:type="dbl:IdExpr" concreteSyntax="trace(&quot;take SEM ERROR&quot;)" referencedElement="//@modules.0/@procedures.0">
                          <arguments>
