      </entities>
      <entities xsi:type="dbl:ProcessDefinition" name="Q" concreteSyntax="process Q;&#xA;&#x9;&#x9;&#x9;timer T3;&#xA;&#xA;&#x9;&#x9;&#x9;start;&#xA;&#x9;&#x9;&#x9;&#x9;set(5,T3);&#xA;&#x9;&#x9;&#x9;nextState K;&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;state K;&#xA;&#x9;&#x9;&#x9;&#x9;input timer T3;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;output M1 to pset P;&#xA;&#x9;&#x9;&#x9;&#x9;nextState K;&#xA;&#x9;&#x9;&#x9;endstate K;&#xA;&#x9;&#x9;endprocess Q;" endNameRef="//@modules.0/@extensions.0/@entities.2">
        <entities xsi:type="dbl:TimerDefinition" name="T3"/>
        <start nextState="//@modules.0/@extensions.0/@entities.2/@states.0">
