      </entities>
      <entities xsi:type="dbl:ProcessDefinition" name="ST" concreteSyntax="process ST;&#xA;&#x9;&#x9;&#x9;dcl int i2=0;&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;start;&#xA;&#x9;&#x9;&#x9;&#x9;take SEM with NO_WAIT;&#xA;&#x9;&#x9;&#x9;&#x9;take SEM with NO_WAIT,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;on OK {&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;task { trace(&quot;take SEM OK&quot;); }&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;give SEM;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;},&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;on ERROR {&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;task { trace(&quot;take SEM ERROR&quot;); }&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;give SEM;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;}&#xA;&#x9;&#x9;&#x9;&#x9;;&#xA;&#x9;&#x9;&#x9;&#x9;take SEM with timeout=10;&#xA;&#x9;&#x9;&#x9;&#x9;take SEM with timeout=10,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;on ERROR {&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;}&#xA;&#x9;&#x9;&#x9;&#x9;;&#xA;&#x9;&#x9;&#x9;&#x9;take SEM FOREVER;&#xA;&#x9;&#x9;&#x9;&#x9;give SEM;&#xA;&#x9;&#x9;&#x9;stop;&#xA;&#x9;&#x9;endprocess ST;" endNameRef="//@modules.0/@extensions.0/@entities.4">
        <entities xsi:type="dbl:VariableDefinition">
