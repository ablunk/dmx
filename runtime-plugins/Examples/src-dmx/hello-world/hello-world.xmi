<?xml version="1.0" encoding="ASCII"?>
<dbl:Model xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dbl="http://www.informatik.hu-berlin.de/sam/dbl">
  <module name="hello_world">
    <classifiers xsi:type="dbl:Class" name="Greeter">
      <attributes name="helloGreeting">
        <primitiveType xsi:type="dbl:StringType"/>
      </attributes>
      <constructors>
        <statements xsi:type="dbl:Assignment">
          <variable>
            <idExpr referencedElement="//@module/@classifiers.0/@attributes.0"/>
          </variable>
          <value xsi:type="dbl:StringLiteral" value="hello"/>
        </statements>
      </constructors>
      <constructors>
        <statements xsi:type="dbl:Assignment">
          <variable>
            <idExpr referencedElement="//@module/@classifiers.0/@attributes.0"/>
          </variable>
          <value xsi:type="dbl:IdExpr" referencedElement="//@module/@classifiers.0/@constructors.1/@parameters.0"/>
        </statements>
        <parameters name="helloGreeting_">
          <primitiveType xsi:type="dbl:StringType"/>
        </parameters>
      </constructors>
      <methods name="sayHelloTo">
        <primitiveType xsi:type="dbl:VoidType"/>
        <statements xsi:type="dbl:Print">
          <outputs xsi:type="dbl:Plus">
            <op1 xsi:type="dbl:Plus">
              <op1 xsi:type="dbl:Plus">
                <op1 xsi:type="dbl:IdExpr" referencedElement="//@module/@classifiers.0/@attributes.0"/>
                <op2 xsi:type="dbl:StringLiteral" value=" "/>
              </op1>
              <op2 xsi:type="dbl:IdExpr" referencedElement="//@module/@classifiers.0/@methods.0/@parameters.0"/>
            </op1>
            <op2 xsi:type="dbl:StringLiteral" value="\n"/>
          </outputs>
        </statements>
        <parameters name="receiver">
          <primitiveType xsi:type="dbl:StringType"/>
        </parameters>
      </methods>
    </classifiers>
    <functions name="main">
      <primitiveType xsi:type="dbl:VoidType"/>
      <statements xsi:type="dbl:Variable" name="greeter">
        <classifierType referencedElement="//@module/@classifiers.0"/>
      </statements>
      <statements xsi:type="dbl:Assignment">
        <variable>
          <idExpr referencedElement="//@module/@functions.0/@statements.0"/>
        </variable>
        <value xsi:type="dbl:CreateObject">
          <classifierType referencedElement="//@module/@classifiers.0">
            <callPart/>
          </classifierType>
        </value>
      </statements>
      <statements xsi:type="dbl:FunctionCall">
        <callIdExpr referencedElement="//@module/@classifiers.0/@methods.0">
          <parentIdExpr referencedElement="//@module/@functions.0/@statements.0"/>
          <callPart>
            <callArguments xsi:type="dbl:StringLiteral" value="world"/>
          </callPart>
        </callIdExpr>
      </statements>
    </functions>
  </module>
</dbl:Model>
