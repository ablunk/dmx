#import "../../dbl"

module extensioncompositionpolymorph;

// an extension D which can be composed of another extension B or of DParts
extension D extends dbl ClassContentExtension {
	start DR;
	DR -> "xd" "{" part:Part "}";
	Part -> ;
	Part -> DPart Part;
	Part -> B Part;
	DPart -> "dpart" ";";
}

extension B extends dbl SimpleStatement {
	start BR;
	BR -> "xb" ";";
}