<?xml version="1.0" encoding="ASCII"?>
<dbl:Model xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dbl="http://www.informatik.hu-berlin.de/sam/dbl">
  <imports file="expand">
    <model href="expand.xmi#/"/>
  </imports>
  <module name="expandTest">
    <classes name="A" concreteSyntax="class A { int i=2; }">
      <attributes name="i" concreteSyntax="int i=2">
        <primitiveType xsi:type="dbl:IntType"/>
        <initialValue xsi:type="dbl:IntLiteral" concreteSyntax="2" value="2"/>
      </attributes>
    </classes>
    <functions name="main">
      <primitiveType xsi:type="dbl:VoidType"/>
      <statements xsi:type="dbl:Variable" name="a" concreteSyntax="A a = new A()">
        <classifierType concreteSyntax="A" referencedElement="//@module/@classes.0"/>
        <initialValue xsi:type="dbl:CreateObject" concreteSyntax="new A()">
          <classifierType concreteSyntax="A()" referencedElement="//@module/@classes.0">
            <callPart/>
          </classifierType>
        </initialValue>
      </statements>
      <statements xsi:type="dbl:Print" concreteSyntax="print a.i">
        <outputs xsi:type="dbl:IdExpr" concreteSyntax="a.i" referencedElement="//@module/@classes.0/@attributes.0">
          <parentIdExpr concreteSyntax="a" referencedElement="//@module/@functions.0/@statements.0"/>
        </outputs>
      </statements>
    </functions>
  </module>
</dbl:Model>
