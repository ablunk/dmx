void testFor() {
	int i;
	for (i=1; i < 4; i=i+1) {
		print i;
	}

	for (int j=1; j< 4; j=j+1) {
		print j;
	}
	
	for (i=1; i<2; i=i+1)
		print i;

	for (i=1; i<2; i=i+1)
		for (int j=1; j<2; j=j+1)
			print i+j;
}

void testArray() {
	int array[][] m = new int array[2][1];
